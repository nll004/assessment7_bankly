{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email",
        "phone"
    ],
    "properties": {
        "username": {
            "type": "string",
            "maxLength" : 16,
            "title": "Username Schema"
        },
        "password": {
            "type": "string",
            "minLength": 8,
            "title": "The password Schema",
            "examples": [
                "password123"
            ]
        },
        "first_name": {
            "type": "string",
            "title": "The first_name Schema",
            "examples": [
                "Nate"
            ]
        },
        "last_name": {
            "type": "string",
            "title": "The last_name Schema",
            "examples": [
                "Diaz"
            ]
        },
        "email": {
            "type": "string",
            "format": "email",
            "title": "The email Schema",
            "examples": [
                "email@gmail.com"
            ]
        },
        "phone": {
            "type": "string",
            "pattern": "^(\\([0-9]{3}\\))?[0-9]{3}-[0-9]{4}",
            "title": "The phone Schema",
            "examples": [
                "31512451462"
            ]
        }
    },
    "examples": [{
        "username": "nll0394",
        "password": "password",
        "first_name": "Nate",
        "last_name": "Diaz",
        "email": "email@gmail.com",
        "phone": "31512451462"
    }]
}
